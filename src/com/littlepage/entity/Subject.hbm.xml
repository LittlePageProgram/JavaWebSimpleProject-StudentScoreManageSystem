<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.littlepage.entity">
    <class name="Subject" table="t_subject">
        <id name="subjectId" column="subjectId">
            <generator class="identity">
            </generator>
        </id>
        <property name="snum" column="snum" ></property>
        <property name="sname" column="sname"></property>
        <property name="scid" column="scid"></property>
        <property name="studyScore" column="studyScore"></property>
    </class>
</hibernate-mapping>
